<script>

function btntest1_onclick() 
{

  var info = 1;
  $.get('/setViz',JSON.stringify(info))
      .fail(function() {
        console.log("Failed")
        alert('Request failed: /LoadData');
      });
    window.location.href = "trendline.html";
}

